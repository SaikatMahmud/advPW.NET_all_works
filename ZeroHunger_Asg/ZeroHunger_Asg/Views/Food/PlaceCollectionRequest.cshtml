
@{
    ViewBag.Title = "PlaceCollectionRequest";
}

<h2>Place A Collection Request</h2>
<button class="pull-right btn btn-warning"><a href="/User/Logout">Logout</a></button>

<button><a href="/Food/RestaurantHistory">Your History</a></button>
<form action="" method="post">
    Food Type:
    <input type="text" name="FoodType" value="" class="form-control" /> <br />
    Quantity (for person):
    <input type="text" name="Quantity" value="" class="form-control" /> <br />
    Maximum Preserve Time (hour):
    <input type="number" name="PreserveTime" value="" class="form-control" /><br />
    <input type="submit" value="Sent Request" />
</form>
